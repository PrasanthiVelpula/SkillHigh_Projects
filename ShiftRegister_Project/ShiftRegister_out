#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01208680 .scope module, "shftregtb" "shftregtb" 2 1;
 .timescale 0 0;
v0120BAE8_0 .var "clk", 0 0;
v0120A930_0 .var "dir", 0 0;
v0120A988_0 .var "ld", 0 0;
v0120A9E0_0 .var "mode", 0 0;
v0120AA38_0 .var "pa_in", 3 0;
v0120AA90_0 .net "pa_out", 3 0, v01203450_0; 1 drivers
v01240758_0 .var "rst", 0 0;
v01240968_0 .var "se_in", 0 0;
v01240390_0 .net "se_out", 0 0, v0120B9E0_0; 1 drivers
v012405A0_0 .var "shft_en", 0 0;
v01240230_0 .net "shft_reg", 3 0, v0120BA90_0; 1 drivers
S_01208818 .scope module, "uut" "ShiftRegister" 2 11, 3 1, S_01208680;
 .timescale 0 0;
v0120E5F8_0 .net "clk", 0 0, v0120BAE8_0; 1 drivers
v01242F68_0 .net "dir", 0 0, v0120A930_0; 1 drivers
v01242FC0_0 .net "ld", 0 0, v0120A988_0; 1 drivers
v01243018_0 .net "mode", 0 0, v0120A9E0_0; 1 drivers
v012033F8_0 .net "pa_in", 3 0, v0120AA38_0; 1 drivers
v01203450_0 .var "pa_out", 3 0;
v012034A8_0 .net "rst", 0 0, v01240758_0; 1 drivers
v0120B988_0 .net "se_in", 0 0, v01240968_0; 1 drivers
v0120B9E0_0 .var "se_out", 0 0;
v0120BA38_0 .net "shft_en", 0 0, v012405A0_0; 1 drivers
v0120BA90_0 .var "shft_reg", 3 0;
E_0120A210 .event posedge, v012034A8_0, v0120E5F8_0;
    .scope S_01208818;
T_0 ;
    %wait E_0120A210;
    %load/v 8, v012034A8_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v0120BA90_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0120B9E0_0, 0, 0;
    %ix/load 0, 4, 0;
    %assign/v0 v01203450_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v01242FC0_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v012033F8_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0120BA90_0, 0, 8;
    %jmp T_0.3;
T_0.2 ;
    %load/v 8, v0120BA38_0, 1;
    %jmp/0xz  T_0.4, 8;
    %load/v 8, v01243018_0, 1;
    %cmpi/u 8, 0, 1;
    %jmp/0xz  T_0.6, 4;
    %load/v 8, v01242F68_0, 1;
    %cmpi/u 8, 0, 1;
    %jmp/0xz  T_0.8, 4;
    %load/v 8, v0120BA90_0, 1; Only need 1 of 4 bits
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v0120B9E0_0, 0, 8;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.10, 4;
    %load/x1p 12, v0120BA90_0, 3;
    %jmp T_0.11;
T_0.10 ;
    %mov 12, 2, 3;
T_0.11 ;
    %mov 8, 12, 3; Move signal select into place
    %mov 11, 0, 1;
    %ix/load 0, 4, 0;
    %assign/v0 v0120BA90_0, 0, 8;
    %jmp T_0.9;
T_0.8 ;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.12, 4;
    %load/x1p 8, v0120BA90_0, 1;
    %jmp T_0.13;
T_0.12 ;
    %mov 8, 2, 1;
T_0.13 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v0120B9E0_0, 0, 8;
    %mov 8, 0, 1;
    %load/v 9, v0120BA90_0, 3; Select 3 out of 4 bits
    %ix/load 0, 4, 0;
    %assign/v0 v0120BA90_0, 0, 8;
T_0.9 ;
    %jmp T_0.7;
T_0.6 ;
    %load/v 8, v01242F68_0, 1;
    %cmpi/u 8, 0, 1;
    %jmp/0xz  T_0.14, 4;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.16, 4;
    %load/x1p 12, v0120BA90_0, 3;
    %jmp T_0.17;
T_0.16 ;
    %mov 12, 2, 3;
T_0.17 ;
    %mov 8, 12, 3; Move signal select into place
    %load/v 11, v0120B988_0, 1;
    %ix/load 0, 4, 0;
    %assign/v0 v0120BA90_0, 0, 8;
    %jmp T_0.15;
T_0.14 ;
    %load/v 8, v0120B988_0, 1;
    %load/v 9, v0120BA90_0, 3; Select 3 out of 4 bits
    %ix/load 0, 4, 0;
    %assign/v0 v0120BA90_0, 0, 8;
T_0.15 ;
T_0.7 ;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %load/v 8, v0120BA90_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v01203450_0, 0, 8;
    %jmp T_0;
    .thread T_0;
    .scope S_01208680;
T_1 ;
    %delay 5, 0;
    %load/v 8, v0120BAE8_0, 1;
    %inv 8, 1;
    %set/v v0120BAE8_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_01208680;
T_2 ;
    %vpi_call 2 29 "$dumpfile", "ShiftRegister.vcd";
    %vpi_call 2 30 "$dumpvars", 1'sb0, S_01208680;
    %end;
    .thread T_2;
    .scope S_01208680;
T_3 ;
    %vpi_call 2 35 "$monitor", "Time=%0t : clk = %b : Mode=%b : Dir=%b : Ld=%b : ShftEn=%b : se_in=%b : pa_in=%b : pa_out=%b : se_out=%b : shftreg=%b", $time, v0120BAE8_0, v0120A9E0_0, v0120A930_0, v0120A988_0, v012405A0_0, v01240968_0, v0120AA38_0, v0120AA90_0, v01240390_0, v01240230_0;
    %set/v v0120BAE8_0, 0, 1;
    %set/v v01240758_0, 1, 1;
    %set/v v0120A988_0, 0, 1;
    %set/v v012405A0_0, 0, 1;
    %set/v v0120A9E0_0, 0, 1;
    %set/v v0120A930_0, 0, 1;
    %set/v v01240968_0, 0, 1;
    %set/v v0120AA38_0, 0, 4;
    %delay 10, 0;
    %set/v v01240758_0, 0, 1;
    %set/v v0120A9E0_0, 0, 1;
    %set/v v0120A988_0, 1, 1;
    %movi 8, 11, 4;
    %set/v v0120AA38_0, 8, 4;
    %delay 10, 0;
    %set/v v0120A988_0, 0, 1;
    %set/v v012405A0_0, 1, 1;
    %set/v v0120A930_0, 0, 1;
    %delay 40, 0;
    %set/v v01240758_0, 1, 1;
    %delay 10, 0;
    %set/v v01240758_0, 0, 1;
    %set/v v0120A9E0_0, 1, 1;
    %set/v v0120A930_0, 1, 1;
    %set/v v012405A0_0, 1, 1;
    %set/v v01240968_0, 1, 1;
    %delay 10, 0;
    %set/v v01240968_0, 1, 1;
    %delay 10, 0;
    %set/v v01240968_0, 0, 1;
    %delay 10, 0;
    %set/v v01240968_0, 1, 1;
    %delay 10, 0;
    %vpi_call 2 67 "$finish";
    %end;
    .thread T_3;
